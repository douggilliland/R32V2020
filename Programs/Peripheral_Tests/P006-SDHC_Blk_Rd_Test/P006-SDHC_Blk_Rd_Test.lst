;
; asciiToHex_ANSI_UART - Convert a single ASCII hex character into a nibble
; Make conversion case insensitive
; Character to convert is passed in r8
;	'0' = 0x30
;	'9' = 0x39
;	'A' = 0x41
;	'F' = 0x46
;	'a' = 0x61
;	'f' = 0x66
; Result is returned in r8
;	0x0-0xf - Legal Values
;	'DEAD' - Not hex character
;

asciiToHex_ANSI_UART:
000001b0	39380066	cmpi	r8,0x66			; past 'f'
000001b1	CC000011	blt		a2h_Error
000001b2	39380030	cmpi	r8,0x30			; below '0'
000001b3	CF00000F	bgt		a2h_Error
000001b4	3938003A	cmpi	r8,0x3A			; '0' - '9'
000001b5	CF000009	bgt		gotDigit
000001b6	39380041	cmpi	r8,0x41			; ':' - '@'
000001b7	CF00000B	bgt		a2h_Error
000001b8	39380047	cmpi	r8,0x47			; 'A' - 'F'
000001b9	CC000007	blt		gotUpperLetter
000001ba	39380061	cmpi	r8,0x61			; 'G' - 'tick'
000001bb	CC000007	blt		a2h_Error
; Lower case letter
000001bc	23880057	subi	r8,r8,0x57
000001bd	C000000A	bra		doneConvA2H
; number 0-9
gotDigit:
000001be	23880030	subi	r8,r8,0x30
000001bf	C0000008	bra		doneConvA2H
; A-F
gotUpperLetter:
000001c0	23880037	subi	r8,r8,0x37
000001c1	C0000006	bra		doneConvA2H
a2h_Error:
000001c2	42800068	lix		r8,syntaxError.lower
000001c3	D4FFFFD8	bsr		newLine_ANSI_UART
000001c4	D4FFFFC0	bsr		printString_ANSI_UART
000001c5	D4FFFFD6	bsr		newLine_ANSI_UART
000001c6	4280DEAD	lix		r8,0xDEAD
doneConvA2H:
000001c7	A1704000	pull	PC

