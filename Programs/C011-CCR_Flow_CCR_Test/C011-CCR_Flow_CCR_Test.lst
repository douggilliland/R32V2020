; CMP and Bxx Checks
start:
00000000	38300000		cmp r0,r0
00000001	D200000B		bne failed
00000002	D1000002		beq	skip1
00000003	C0000009		bra	failed
skip1:
00000004	38301000		cmp r0,r1
00000005	D1000007		beq failed
00000006	D2000002		bne	skip2
00000007	C0000005		bra	failed
skip2:
passed:		; send GOOD to the 7 Segment LED
00000008	42503000		lix	PAR,0x3000
00000009	42A0900D		lix	r10,0x900D
0000000a	8550A000		spl	r10
0000000b	01000000		hcf
failed:		; send 0BAD to the 7 Segment LED
0000000c	42503000		lix	PAR,0x3000
0000000d	42A00BAD		lix	r10,0x0BAD
0000000e	8550A000		spl	r10
0000000f	01000000		hcf
