; CMP and Bxx Checks
start:
00000000	30300000		cmp r0,r0
00000001	D200000F		bne failed
00000002	D1000002		beq	skip1
00000003	C000000D		bra	failed
skip1:
00000004	30301000		cmp r0,r1
00000005	D100000B		beq failed
00000006	D2000004		bne	skip2
00000007	C0000009		bra	failed
00000008	C0000002		bra passed
00000009	00000000		nop
skip2:
passed:		; send GOOD to the 7 Segment LED
0000000a	20B00000		add	r11,r0,r0
0000000b	40B03000		lil	r11,0x3000
0000000c	205B0000		add	par,r11,r0
0000000d	40A0900D		lil	r10,0x900D
0000000e	8550A000		spl	r10
0000000f	01000000		hcf
failed:		; send 0BAD to the 7 Segment LED
00000010	20B00000		add	r11,r0,r0
00000011	40B03000		lil	r11,0x3000
00000012	205B0000		add	par,r11,r0
00000013	40A00BAD		lil	r10,0x0BAD
00000014	8550A000		spl	r10
00000015	01000000		hcf
